public Integer secondLargest(List<Integer> numbers) {
    numbers.sort();
    Integer arr1Max = numbers[numbers.size() - 2];
    Integer arr2Max = numbers[numbers.size() - 1];

    if(arr1Max == arr2Max)
    {
        return numbers[numbers.size() - 3];
    }
    return numbers[numbers.size() - 2];
}